module comparator16 (
    input alufn[6],  // 
    input z,
    input v,
    input n,
    output comp
  ) { 
  
  always {
    case(alufn[2:1])
    {
      b00:
        comp = 0;
        
      b01:
        comp = z;
      
      b10:
        comp = n ^ v;
      
      b11:
        comp = z | (n ^ v);
        
      default:
        comp = 0;
  }
}
}